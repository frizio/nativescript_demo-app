<Page class="page" navigatingTo="theFunction">

    <Page.actionBar>
        <ActionBar title="The Demo App" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>

    <AbsoluteLayout>
        <Label text="{{message}}" textWrap="true"/>
        <button text="Click" tap="clickMe" left="0" top="0"></button>
        <button text="Click 1" tap="clickMe" left="100" top="100"></button>
        <button text="Click 2" tap="clickMe" left="100" top="200"></button>
        <button text="Click 3" tap="clickMe" left="200" top="100"></button>
        <button text="Click 4" tap="clickMe" left="200" top="200"></button>
    </AbsoluteLayout>
</Page>
